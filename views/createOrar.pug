                                            label.form-check-label(for="Seminar") S
                                        .form-check.form-check-inline
                                            input.form-check-input(type="radio", name="tipluni9" value="Proiect" )
                                            label.form-check-label(for="Proiect") P
                                    .col
                                        .form-group.m-1
                                            select.sala.form-control(name="salaluni")
                                                if sali != null 
                                                    each s in sali
                                                        if s.etaj == 0 
                                                            option #{s.corp}P#{s.numar}
                                                        else
