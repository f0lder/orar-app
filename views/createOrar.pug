extends layout

block content 

    form(method="post" action="/orar/insertOrar")
        .d-flex.flex-row.align-self-center.m-3
            h3.px-3 Creaza orar pentru grupa
            .w-5
                select.form-control#grupa(name="grupa" placeholder="")
                    each g in grupe
                        option #{g.cod}
    
        .container-fluid
            table.table.table-bordered.tabel-orar
                thead
                    tr
                        th(scope="col" style="width: 5%") 
                            h2.text-center Ore
                        th(scope="col")
                            h2.text-center Luni
                        th(scope="col")
                            h2.text-center Marti
                        th(scope="col")
                            h2.text-center Miercuri
                        th(scope="col")
                            h2.text-center Joi
                        th(scope="col")
                            h2.text-center Vineri
                tbody
                    tr
                        th(scope="row").text-center 9:00-11:00
