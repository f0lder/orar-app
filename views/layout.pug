	body
		nav.navbar.navbar-expand-lg.navbar.navbar-dark.bg-primary
			.container-fluid
				a.navbar-brand(href="/")
					.row
						.col
							img(src="/images/logo.svg", width="45", height="45", class="d-inline-block align-text-top")
						.col.d-flex.align-items-center
							p.m-0 Orare
				if loggedin
					ul.navbar-nav
						li.nav-item.active
							a.nav-link.active(href="/") Index
						li.nav-item
							a.nav-link(href="/home") Dashboard
						li.nav-item
							a.nav-link(href="/orar") Creaza Orare
						li.nav-item.dropdown
							a.nav-link.dropdown-toggle#menuD(href="#" role="button" data-bs-toggle="dropdown" aria-expanded="false") Date
							ul.dropdown-menu(aria-labelledby="menuD")
								li
									a.dropdown-item(href="/sali") Sali
								li 
									a.dropdown-item(href="/grupe") Grupe
								li 
									a.dropdown-item(href="/materii") Materii
								li 
									a.dropdown-item(href="/orar/all") Orare
								li 
									a.dropdown-item(href="/profi") Profesori
								li 
									a.dropdown-item(href="/insertData") Insert Data(test page)
						li.nav-item
							a.nav-link(href="/logout")
								svg.bi.bi-box-arrow-right(xmlns="http://www.w3.org/2000/svg", width="22", height="22", fill="currentColor", viewBox="0 0 16 16")
									path(fill-rule="evenodd", d="M10 12.5a.5.5 0 0 1-.5.5h-8a.5.5 0 0 1-.5-.5v-9a.5.5 0 0 1 .5-.5h8a.5.5 0 0 1 .5.5v2a.5.5 0 0 0 1 0v-2A1.5 1.5 0 0 0 9.5 2h-8A1.5 1.5 0 0 0 0 3.5v9A1.5 1.5 0 0 0 1.5 14h8a1.5 1.5 0 0 0 1.5-1.5v-2a.5.5 0 0 0-1 0z")
									path(fill-rule="evenodd", d="M15.854 8.354a.5.5 0 0 0 0-.708l-3-3a.5.5 0 0 0-.708.708L14.293 7.5H5.5a.5.5 0 0 0 0 1h8.793l-2.147 2.146a.5.5 0 0 0 .708.708z")
		.main
			block content
		.blue
			.container
				footer
					.row.d-flex.justify-content-end.border-top.py-5.mt-5
						.col
							h5 Universitatea "Vasile Alecsandri" din Bacău
							ul.nav.flex-column 
								li.nav-item.mb-2 Facultatea de Inginerie
								li.nav-tem.mb-2 Specializarea CTI
						.col-3
							h5 Proiect realizat de:
							ul.nav.flex-column
								li.nav-item.mb-2 Ursan Bogdan-Gabriel 
								li.nav-item.mb-2 Drăgoescu Ștefania-Andreea
								li.nav-item.mb-2 Nițoi Razvan-Mihăiță

		block scripts
			script(src="javascripts/active.js")
			script(src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/js/bootstrap.bundle.min.js" integrity="sha384-MrcW6ZMFYlzcLA8Nl+NtUVF0sA7MsXsP1UyJoMp4YLEuNSfAP+JcXn/tWtIaxVXM" crossorigin="anonymous")