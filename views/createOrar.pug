                                    path(d="M8 4a.5.5 0 0 1 .5.5v3h3a.5.5 0 0 1 0 1h-3v3a.5.5 0 0 1-1 0v-3h-3a.5.5 0 0 1 0-1h3v-3A.5.5 0 0 1 8 4")
                            .d-none#luni9
                                .row.m-1
                                    .col
                                        .form-group
                                            select.mat.form-control(name="luni")
                                                option(value=null) Liber
                                                if materii != null 
                                                    each m in materii
                                                        option #{m.nume} 
                                .row.m-1
                                    .col
                                        .form-check.form-check-inline
                                            input.form-check-input(type="radio", name="tipluni9" value="Curs"  checked)
                                            label.form-check-label(for="Curs") C
                                        .form-check.form-check-inline
